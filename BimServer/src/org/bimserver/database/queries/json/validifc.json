{
	defines: {
		Units: {
			type: "IfcProject",
			field: "UnitsInContext",
			include: {
				type: "IfcUnitAssignment",
				field: "Units",
				includes: ["ConversionFactor", "Dimensions"]
			}
		},
		ContainedInStructure: {
			type: "IfcElement",
			field: "ContainedInStructure",
			includes: [{
				type: "IfcRelContainedInSpatialStructure",
				field: "RelatingStructure",
				includes: ["Decomposes", "OwnerHistory"]
			}, "OwnerHistory"]
		},
		Dimensions: {
			type: "IfcConversionBasedUnit",
			field: "Dimensions"
		},
		ConversionFactor: {
			type: "IfcConversionBasedUnit",
			field: "ConversionFactor",
			include: {
				type: "IfcMeasureWithUnit",
				field: "UnitComponent"
			}
		},
		Decomposes: {
			type: "IfcObjectDefinition",
			field: "Decomposes",
			includes: [{
				type: "IfcRelDecomposes",
				field: "RelatingObject",
				includes: ["Decomposes", "Units", "OwnerHistory"]
			}, "OwnerHistory"]
		},
		OwnerHistory: {
			type: "IfcRoot",
			field: "OwnerHistory",
			includes: [{
				type: "IfcOwnerHistory",
				field: "OwningUser",
				includes: [{
					type: "IfcPersonAndOrganization",
					field: "ThePerson"
				}, {
					type: "IfcPersonAndOrganization",
					field: "TheOrganization"
				}]
			}, {
				type: "IfcOwnerHistory",
				field: "OwningApplication",
				include: {
					type: "IfcApplication",
					field: "ApplicationDeveloper"
				}
			}]
		},
		Representation: {
			type: "IfcProduct",
			field: "Representation",
			include: {
				type: "IfcProductRepresentation",
				field: "Representations",
				include: {
					type: "IfcRepresentation",
					field: "Items",
					includes: [{
						type: "IfcPolyline",
						field: "Points"
					}, {
						type: "IfcExtrudedAreaSolid",
						field: "ExtrudedDirection"
					}, {
						type: "IfcExtrudedAreaSolid",
						field: "Position",
						include: {
							type: "IfcAxis2Placement3D",
							fields: ["Location", "Axis", "RefDirection"]
						}
					}, {
						type: "IfcExtrudedAreaSolid",
						field: "SweptArea",
						include: {
							type: "IfcArbitraryClosedProfileDef",
							field: "OuterCurve",
							include: {
								type: "IfcPolyline",
								field: "Points"
							}
						}
					}]
				}
			}
		},
		RelativePlacement: {
			type: "IfcLocalPlacement",
			field: "PlacementRelTo",
			includes: ["RelativePlacement", {
				type: "IfcLocalPlacement",
				field: "RelativePlacement",
				include: {
					type: "IfcAxis2Placement3D",
					fields: ["Location", "Axis", "RefDirection"]
				}
			}]
		},
		ObjectPlacement: {
			type: "IfcProduct",
			field: "ObjectPlacement",
			includes: ["RelativePlacement", {
				type: "IfcLocalPlacement",
				field: "RelativePlacement",
				include: {
					type: "IfcAxis2Placement3D",
					fields: ["Location", "Axis", "RefDirection"]
				}
			}]
		}
	}
}